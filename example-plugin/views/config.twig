{% extends 'admin.base' %}

{% block title %}示例插件的配置{% endblock %}

{% block content %}
  <div class="row">
    <div class="col-lg-12">
      {{ form|raw }}
    </div>
  </div>

  <div class="row">
    <div class="col-lg-12">
      <div class="card">
        <div class="card-header">
          <div class="card-title">README.md（此文件在插件根目录下）</div>
        </div>
        <div class="card-body">
          {{ readme|raw }}
        </div>
      </div>
    </div>
  </div>
{% endblock %}
